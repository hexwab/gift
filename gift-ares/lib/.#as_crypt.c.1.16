/* encrypt/decrypt http download header b6st */
void as_encrypt_b6st (as_uint8 *data, int len)
{
	munge (data, len, 0xB334, 0xCE6D, 0x58BF);
}

void as_decrypt_b6st (as_uint8 *data, int len)
{
	unmunge (data, len, 0xB334, 0xCE6D, 0x58BF);
}

