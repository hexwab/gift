	INVOKE(sub_5F0FF4, buf);
	INVOKE(sub_5EFB14, buf);
	INVOKE(sub_5ECD08, buf);
	INVOKE(sub_5F1D64, buf);
	INVOKE(sub_5EEAD0, buf);
	INVOKE(sub_5F01D8, buf);
	INVOKE(sub_5F251C, buf);
	INVOKE(sub_5F0C40, buf);
